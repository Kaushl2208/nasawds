<!doctype html>
<html lang="{{ lang | default('en') }}">
  <!-- generated by {{ package.name }}@{{ package.version }} -->
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{{ meta.title | default('USWDS Fractal example document') }}</title>

    <!-- Favicons
    ================================================== -->
    <!-- 128x128 -->
    <link rel="shortcut icon" type="image/ico" href="{{ nasawds.path }}/img/favicons/favicon.ico">
    <link rel="icon" type="image/png" href="{{ nasawds.path }}/img/favicons/favicon.png">

    <!-- 192x192, as recommended for Android
    http://updates.html5rocks.com/2014/11/Support-for-theme-color-in-Chrome-39-for-Android
    -->
    <link rel="icon" type="image/png" sizes="192x192" href="{{ nasawds.path }}/img/favicons/favicon-192.png">

    <!-- 57x57 (precomposed) for iPhone 3GS, pre-2011 iPod Touch and older Android devices -->
    <link rel="apple-touch-icon-precomposed" href="{{ nasawds.path }}/img/favicons/favicon-57.png">
    <!-- 72x72 (precomposed) for 1st generation iPad, iPad 2 and iPad mini -->
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ nasawds.path }}/img/favicons/favicon-72.png">
    <!-- 114x114 (precomposed) for iPhone 4, 4S, 5 and post-2011 iPod Touch -->
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ nasawds.path }}/img/favicons/favicon-114.png">
    <!-- 144x144 (precomposed) for iPad 3rd and 4th generation -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ nasawds.path }}/img/favicons/favicon-144.png">

    <link rel="stylesheet" href="{{ nasawds.path }}/css/nasawds.min.css">
    {% block styles %}{% endblock %}
  </head>
  <body>
    {% block skipnav -%}
      <a class="usa-skipnav" href="#main-content">Skip to main content</a>
    {% endblock %}

    {% block banner -%}
      {% render '@banner' %}
    {% endblock %}

    {% block header -%}
      {% render '@header--basic', {header: header}, true %}
    {% endblock %}

    {% block body -%}
      <main id="main-content">
      </main>
    {% endblock %}

    {% block footer -%}
      {% render '@footer' %}
    {% endblock %}

    <script src="{{ nasawds.path }}/js/nasawds.min.js"></script>
    {% block scripts %}{% endblock %}
  </body>
</html>
